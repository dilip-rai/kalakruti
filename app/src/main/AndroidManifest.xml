<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.artwork.kalakruti"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".KalakrutiActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".javadraw.JavaDrawActivity">
        </activity>
        <activity
            android:name=".skiadraw.SkiaDrawActivity"
            android:label="@string/title_activity_skia_draw"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>
